

public class position{
		
	int[] xCoords = {260,380,500,620,740,860};
	int[] yCoords = {13,121,229,337,445,553};
	
	public int[] position(int row,int col){
		intx = xCoords[row];
		inty = yCoords[col];
		}
	
	public int getRow() {
		return row;
	}
	public int getCol() {
		return col;
	}
	public String toString() {
		return "" + row + ", "+col;
	}
	
}
